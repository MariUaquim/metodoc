<h1>Editing doc_type_config</h1>

<% form_for(@doc_type_config) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :answer %><br />
    <%= f.check_box :answer %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @doc_type_config %> |
<%= link_to 'Back', doc_type_configs_path %>
