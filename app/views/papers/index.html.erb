<h1>Listing papers</h1>

<table>
  <tr>
    <th>Paper</th>
  </tr>

<% for papers in @papers %>
  <tr>
    <td><%=h papers.paper %></td>
    <td><%= link_to 'Show', papers %></td>
    <td><%= link_to 'Edit', edit_papers_path(papers) %></td>
    <td><%= link_to 'Destroy', papers, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New papers', new_papers_path %>
